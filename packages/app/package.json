{
  "name": "@nebula/orion",
  "version": "1.4.0",
  "private": true,
  "main": "./dist/_electron/electron.js",
  "homepage": "./",
  "description": "Extraterrestrial automation helping you secure even the most limited items",
  "author": {
    "name": "Matthew Wall",
    "email": "hello@nebulabots.com"
  },
  "scripts": {
    "build": "run-s build:obfuscate build:react",
    "build:react": "cross-env NODE_ENV=production GENERATE_SOURCEMAP=false react-scripts build",
    "build:obfuscate": "node tool/obfuscateLib.js",
    "ci": "cross-env CI=true yarn run test",
    "coverage": "yarn run test --coverage",
    "dev-server": "cross-env HTTPS=true react-scripts start",
    "dev": "cross-env NODE_ENV=development electron ./lib/_electron/electron.js",
    "eject": "react-scripts eject",
    "lint": "eslint --ext js,jsx src/",
    "afterinstall": "electron-builder install-app-deps",
    "clean": "rimraf build dist",
    "compile": "yarn run build",
    "pack": "electron-builder -m --publish never",
    "dist": "electron-builder -mw --publish always",
    "deploy": "run-s clean compile pack",
    "publish": "run-s clean compile dist",
    "prod": "cross-env NODE_ENV=production electron .",
    "compile-sass": "./tool/compile_css.sh",
    "start": "yarn prod",
    "test": "react-scripts test --env=jsdom --verbose=false"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "@manaflair/redux-batch": "^1.0.0",
    "@nebula/task-built": "*",
    "card-validator": "^6.2.0",
    "date-fns": "^1.30.1",
    "discord-rpc": "^3.0.2",
    "dotenv": "^8.0.0",
    "electron-about-window": "^1.13.1",
    "electron-log": "^3.0.6",
    "electron-store": "^3.3.0",
    "electron-updater": "^4.1.2",
    "globalize": "^1.4.2",
    "https-proxy-agent": "^2.2.1",
    "jsonfile": "^5.0.0",
    "lodash": "^4.17.11",
    "node-fetch": "^2.6.0",
    "prop-types": "^15.6.2",
    "ps-list": "^6.3.0",
    "react": "^16.8",
    "react-cool-img": "^1.0.1",
    "react-datetime": "^2.16.3",
    "react-datetime-picker": "^2.8.0",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.12.18",
    "react-loader-spinner": "^2.3.0",
    "react-modal": "^3.11.1",
    "react-modal-image": "^2.5.0",
    "react-number-format": "^4.0.8",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scrollable-feed": "1.0.4",
    "react-select": "^3.0.4",
    "react-switch": "^5.0.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-widgets": "^4.4.11",
    "react-widgets-globalize": "^5.0.20",
    "react-widgets-moment": "^4.0.27",
    "react-window": "^1.8.5",
    "redux": "^4.0.0",
    "redux-ignore": "^1.2.5",
    "redux-localstorage": "^0.4.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sanitize-html": "^1.20.1",
    "semver": "^6.1.1",
    "shortid": "^2.2.14",
    "url-loader": "2.1.0",
    "uuid": "^3.3.2",
    "v8-compile-cache": "2.1.0",
    "whatwg-url": "^7.0.0"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "babel-jest": "24.9.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "6.0.12",
    "electron-builder": "21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "glob": "^7.1.4",
    "history": "^4.7.2",
    "husky": "^2.4.1",
    "javascript-obfuscator": "^0.18.1",
    "jest": "24.9.0",
    "jest-enzyme": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "prettier-eslint": "^9.0.0",
    "react-scripts": "3.2.0",
    "redux-mock-store": "^1.5.3",
    "replace": "^1.1.0",
    "rimraf": "^2.6.3",
    "sass": "1.23.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".eslintrc.js",
      "setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".eslintrc.js",
      "setupTests.js"
    ]
  }
}
