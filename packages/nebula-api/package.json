{
  "name": "@nebula/api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "babel --root-mode upward --no-comments -d build src/",
    "ci": "exit 0",
    "run:dev": "cross-env NODE_ENV=development nodemon src/app.js",
    "dev:setup": "cross-env NODE_ENV=development node dynamoDBSetup.js",
    "dev:put": "cross-env NODE_ENV=development node dynamoDBPutKey.js",
    "dev:put:view": "cross-env NODE_ENV=development node dynamoDBPutKey.js -k",
    "dev:delete": "cross-env NODE_ENV=development node dynamoDBDeleteTables.js",
    "run:prod": "cross-env NODE_ENV=production nodemon src/app.js",
    "prod:setup": "cross-env NODE_ENV=production node dynamoDBSetup.js",
    "prod:put": "cross-env NODE_ENV=production node dynamoDBPutKey.js",
    "prod:put:view:all": "cross-env NODE_ENV=production node dynamoDBPutKey.js -a",
    "prod:put:view:keys": "cross-env NODE_ENV=production node dynamoDBPutKey.js -k",
    "prod:put:view:discord": "cross-env NODE_ENV=production node dynamoDBPutKey.js -d",
    "prod:put:view:users": "cross-env NODE_ENV=production node dynamoDBPutKey.js -u",
    "prod:delete": "cross-env NODE_ENV=production node dynamoDBDeleteTables.js",
    "lint": "eslint src",
    "postinstall": "yarn run build",
    "prestart": "yarn run build",
    "prod": "cross-env NODE_ENV=production nodemon build/app.js",
    "start": "yarn run prod",
    "test": "echo \"Warning: no test specified\" && exit 0"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "aws-sdk": "^2.477.0",
    "cors": "^2.8.4",
    "dotenv": "^8.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.2",
    "moment": "^2.22.2",
    "node-fetch": "^2.6.0",
    "recaptcha-verify": "^0.1.1",
    "stripe": "7.7.0"
  }
}
