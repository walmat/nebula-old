    "test": "echo \"Warning: no test specified\" && exit 0"
  },
  "dependencies": {
    "aws-sdk": "^2.373.0",
    "@babel/runtime": "^7.4.3",
    "eventemitter3": "^3.1.0",
    "shortid": "^2.2.14",
    "ssh2": "^0.6.1",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "javascript-obfuscator": "^0.18.1"
  }
}
