{
  "name": "@nebula/orion",
  "version": "1.0.0",
  "private": true,
  "main": "./lib/_electron/electron.js",
  "homepage": "./",
  "description": "Innovative Shopify Botting",
  "author": {
    "name": "Matthew Wall",
    "email": "hello@nebulabots.com"
  },
  "scripts": {
    "build": "cross-env GENERATE_SOURCEMAP=false react-app-rewired build",
    "ci": "CI=true npm run test",
    "coverage": "npm run test -- --coverage",
    "dev-server": "cross-env HTTPS=true react-app-rewired start",
    "dev": "cross-env NODE_ENV=development electron .",
    "eject": "react-app-rewired eject",
    "lint": "eslint --ext js,jsx src/",
    "pack:linux": "run-s private:clean private:compile private:build:linux",
    "pack:mac": "run-s private:clean private:compile private:build:mac",
    "pack:win": "run-s private:clean private:compile private:build:win",
    "pack": "run-s private:clean private:compile private:build:all",
    "postinstall": "electron-builder install-app-deps",
    "prebuild": "replace 'file-loader' 'url-loader' ../../node_modules/react-scripts/config/webpack.config.prod.js",
    "private:build:all": "electron-builder -mwl",
    "private:build:linux": "electron-builder --linux",
    "private:build:mac": "electron-builder --mac",
    "private:build:win": "electron-builder --win",
    "private:clean": "rimraf build",
    "private:compile": "npm run build",
    "private:serve": "babel-node scripts/serve.js",
    "private:watch": "npm run private:compile -- --source-maps true --watch --skip-initial-build",
    "prod": "cross-env NODE_ENV=production electron .",
    "sass": "./tool/compile_css.sh",
    "start": "npm run prod",
    "test": "react-app-rewired test --env=jsdom"
  },
  "dependencies": {
    "@nebula/task-runner": "^0.1.0",
    "aws-sdk": "^2.315.0",
    "body-parser": "^1.18.3",
    "bodymovin": "^4.13.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "electron-store": "^2.0.0",
    "express": "^4.16.3",
    "moment": "^2.22.2",
    "node-fetch": "^2.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-number-format": "^3.6.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^2.0.0",
    "react-select": "^2.0.0",
    "redux": "^4.0.0",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.3.0",
    "sanitize-html": "^1.18.4",
    "underscore": "^1.9.1",
    "uuid": "^3.3.2",
    "whatwg-url": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.7.13",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "discord.js": "^11.3.2",
    "electron": "^3.0.6",
    "electron-builder": "^20.28.4",
    "electron-devtools-installer": "^2.2.4",
    "electron-icon-maker": "0.0.4",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "history": "^4.7.2",
    "javascript-obfuscator": "^0.18.1",
    "jest": "^23.6.0",
    "jest-enzyme": "^6.1.2",
    "npm-run-all": "^4.1.5",
    "react-app-rewire-yarn-workspaces": "^1.0.3",
    "react-app-rewired": "^1.6.2",
    "redux-mock-store": "^1.5.3",
    "replace": "^1.0.0",
    "rimraf": "^2.6.2",
    "sass": "^1.15.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/tasks/old/*.{js,jsx}",
      "!src/server/old/*.{js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".eslintrc.js"
    ]
  }
}
