{
  "name": "nebula-orion",
  "version": "1.0.0",
  "private": true,
  "main": "./lib/_electron/electron.js",
  "homepage": "./",
  "description": "Innovative Shopify Botting",
  "author": {
    "name": "Matthew Wall",
    "email": "hello@nebulabots.com"
  },
  "scripts": {
    "start": "npm run prod",
    "prebuild": "./node_modules/.bin/replace 'file-loader' 'url-loader' ./node_modules/react-scripts/config/webpack.config.prod.js",
    "precommit": "lint-staged && npm test -- --coverage",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "coverage": "npm run test -- --coverage",
    "eject": "react-scripts eject",
    "dev-server": "cross-env HTTPS=true react-scripts start",
    "dev": "cross-env NODE_ENV=development electron .",
    "prod": "cross-env NODE_ENV=production electron .",
    "pretty": "prettier --write --tab-width 2 \"src/**/*.js\"",
    "lint": "./node_modules/.bin/eslint src/",
    "sass": "./tool/compile_css.sh",
    "postinstall": "electron-builder install-app-deps",
    "format": "npm run private:format -- --write",
    "pack": "run-s private:clean private:compile private:build:all",
    "pack:mac": "run-s private:clean private:compile private:build:mac",
    "pack:win": "run-s private:clean private:compile private:build:win",
    "pack:linux": "run-s private:clean private:compile private:build:linux",
    "private:build:all": "electron-builder -mwl",
    "private:build:mac": "electron-builder --mac",
    "private:build:win": "electron-builder --win",
    "private:build:linux": "electron-builder --linux",
    "private:watch": "npm run private:compile -- --source-maps true --watch --skip-initial-build",
    "private:serve": "babel-node scripts/serve.js",
    "private:compile": "npm run build",
    "private:clean": "rimraf build",
    "private:format": "prettier \"scripts/*.js\" \"app/**/*.js\" \"test/**/*.js\""
  },
  "dependencies": {
    "aws-sdk": "^2.315.0",
    "body-parser": "^1.18.3",
    "bodymovin": "^4.13.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "electron-store": "^2.0.0",
    "express": "^4.16.3",
    "moment": "^2.22.2",
    "node-fetch": "^2.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-number-format": "^3.6.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^2.0.0",
    "react-select": "^2.0.0",
    "redux": "^4.0.0",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.3.0",
    "sanitize-html": "^1.18.4",
    "task-runner": "file:../task-runner",
    "underscore": "^1.9.1",
    "uuid": "^3.3.2",
    "whatwg-url": "^7.0.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "discord.js": "^11.3.2",
    "electron": "^3.0.6",
    "electron-builder": "^20.28.4",
    "electron-devtools-installer": "^2.2.4",
    "electron-icon-maker": "0.0.4",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "history": "^4.7.2",
    "husky": "^1.1.2",
    "javascript-obfuscator": "^0.18.1",
    "jest": "^23.6.0",
    "jest-enzyme": "^6.1.2",
    "lint-staged": "^7.3.0",
    "npm-run-all": "^4.1.3",
    "redux-mock-store": "^1.5.3",
    "replace": "^1.0.0",
    "rimraf": "^2.6.2",
    "sass": "^1.12.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/tasks/old/*.{js,jsx}",
      "!src/server/old/*.{js,jsx}"
    ]
  },
  "lint-staged": {
    "*.jsx": [
      "npm run pretty",
      "git add"
    ],
    "*.js": [
      "npm run pretty",
      "git add"
    ]
  }
}
